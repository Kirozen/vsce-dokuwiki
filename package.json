{
    "name": "dokuwiki",
    "displayName": "DokuWiki",
    "description": "Syntax highlighting for DokuWiki markup",
    "version": "1.4.0",
    "publisher": "kirozen",
    "icon": "logo/dokuwiki-logo.svg",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dokuwiki",
                "aliases": [
                    "DokuWiki",
                    "dokuwiki"
                ],
                "extensions": [
                    ".txt",
                    ".dokuwiki"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dokuwiki",
                "scopeName": "text.html.dokuwiki",
                "path": "./syntaxes/dokuwiki.tmLanguage"
            }
        ]
    }
}